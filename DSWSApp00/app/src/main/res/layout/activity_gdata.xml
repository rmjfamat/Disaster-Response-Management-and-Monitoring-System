<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFDCFEFC"
    tools:context=".Gdata">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        mapbox:mapbox_cameraTargetLat="10.2930"
        mapbox:mapbox_cameraTargetLng="123.966"
        mapbox:mapbox_cameraZoom="15" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">

        <EditText
            android:id="@+id/search"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#83F1E6EA"
            android:gravity="center"
            android:hint="Search family name"
            android:textColor="#000"
            android:layout_marginRight="100dp"
             />

        <Button
            android:id="@+id/searchBtn"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:background="#DAF1E6EA"
            android:gravity="center"
            android:text="Search"
            android:textColor="#000"
            mapbox:layout_constraintLeft_toRightOf="@+id/search" />

    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>
